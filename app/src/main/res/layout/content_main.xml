<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/main_frame_layout_id"
        tools:context=".Presearch_Fragment">


    <RelativeLayout
        android:layout_marginTop="55dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
      <EditText
          android:id="@+id/base_url_id"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:hint="enter base url"/>

    <EditText
        android:id="@+id/redirect_url_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="enter website you want to redirect"
        android:layout_below="@+id/base_url_id" />

        <Button
            android:id="@+id/generate_redirect_url_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/redirect_url_id"
            android:text="Generate"/>


        <EditText
            android:id="@+id/generate_url_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=".."
            android:layout_below="@+id/generate_redirect_url_id" />

        <Button
            android:id="@+id/generate_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="go to"
            android:layout_below="@+id/generate_url_id" />
        </RelativeLayout>

    </FrameLayout>
